#Laravel
alias art="php artisan"
alias ptf="t --filter "
alias pt="t "
alias lp="laravel"
alias serve='art serve'
alias tinker='art tinker'

t() {
  if [ -f vendor/bin/phpunit ]; then
    vendor/bin/phpunit "$@"
  else
    phpunit "$@"
  fi
}